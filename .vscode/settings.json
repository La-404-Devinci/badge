{
    "editor.tabSize": 2,
    "editor.indentSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "tailwindCSS.experimental.classRegex": [
        ["([\"'`][^\"'`]*.*?[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
    ],
    "tailwindCSS.classAttributes": ["class", "className", ".*ClassName"],
    "i18n-ally.localesPaths": [
        "lib/i18n",
        "app/api/locale",
        "lib/i18n/messages"
    ]
}

{
  "name": "minimal-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker": "./docker/start.sh",
    "dev": "concurrently --raw --kill-others pnpm:dev:*",
    "dev:next": "next dev --turbo",
    "dev:trigger": "npx trigger.dev@latest dev",
    "dev:db": "pnpm db:studio",
    "dev:email": "email dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:start": "docker compose -f docker-compose.yml up -d",
    "db:stop": "docker compose -f docker-compose.yml down -v",
    "db:seed": "tsx db/seed/reset.ts && tsx db/seed/index.ts",
    "db:reset": "tsx db/seed/reset.ts",
    "generate:auth": "pnpx @better-auth/cli generate -y --output ./db/schema/auth-schema.ts && prettier --write ./db/schema/auth-schema.ts",
    "trigger:deploy": "pnpx trigger.dev@latest deploy",
    "precommit": "pnpm typecheck && pnpm lint && pnpm format",
    "env:pull": "vercel env pull .env"
  },
  "dependencies": {
    "@arcjet/next": "1.0.0-beta.5",
    "@hookform/resolvers": "^5.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@react-email/components": "^0.0.36",
    "@remixicon/react": "^4.6.0",
    "@t3-oss/env-nextjs": "^0.13.0",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.74.6",
    "@tanstack/react-table": "^8.21.3",
    "@trigger.dev/sdk": "^3.3.17",
    "@trpc/client": "^11.1.1",
    "@trpc/server": "^11.1.1",
    "@trpc/tanstack-react-query": "^11.1.1",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.8",
    "@vercel/blob": "^1.0.1",
    "ai": "^4.3.9",
    "better-auth": "^1.2.7",
    "client": "link:@vercel/blob/client",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.1.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.7.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "fast-deep-equal": "^3.1.3",
    "isolated-vm": "^5.0.4",
    "merge-refs": "^1.3.0",
    "motion": "^12.8.0",
    "next": "15.3.1",
